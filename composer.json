{
    "$schema": "https://getcomposer.org/schema.json",
    "name": "sheenazien8/framee",
    "type": "project",
    "description": "Modern photo kiosk application with camera integration, frame selection, and payment processing built with Laravel and Vue.js",
    "keywords": [
        "laravel",
        "vue",
        "photo-kiosk",
        "camera",
        "image-processing",
        "payment-gateway",
        "inertia",
        "tailwind",
        "intervention-image",
        "qr-code",
        "midtrans",
        "xendit"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Sheena Zien",
            "email": "sheenazien8@gmail.com"
        }
    ],
    "homepage": "https://github.com/sheenazien8/framee",
    "support": {
        "issues": "https://github.com/sheenazien8/framee/issues",
        "source": "https://github.com/sheenazien8/framee"
    },
    "require": {
        "php": "^8.2",
        "inertiajs/inertia-laravel": "^2.0",
        "intervention/image": "^3.11",
        "laravel/breeze": "^2.3",
        "laravel/framework": "^12.0",
        "laravel/sanctum": "^4.0",
        "laravel/tinker": "^2.10.1",
        "simplesoftwareio/simple-qrcode": "^4.2",
        "spatie/laravel-permission": "^6.21",
        "tightenco/ziggy": "^2.0"
    },
    "require-dev": {
        "fakerphp/faker": "^1.23",
        "laravel/pail": "^1.2.2",
        "laravel/pint": "^1.13",
        "laravel/sail": "^1.41",
        "mockery/mockery": "^1.6",
        "nunomaduro/collision": "^8.6",
        "pestphp/pest": "^3.8",
        "pestphp/pest-plugin-laravel": "^3.2"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "@php artisan config:clear",
            "@php artisan clear-compiled",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi",
            "@php -r \"file_exists('database/database.sqlite') || touch('database/database.sqlite');\"",
            "@php artisan migrate --graceful --ansi"
        ],
        "dev": [
            "Composer\\Config::disableProcessTimeout",
            "npx concurrently -c \"#93c5fd,#c4b5fd,#fb7185,#fdba74\" \"php artisan serve --host=0.0.0.0 --port=8000\" \"php artisan queue:listen --tries=1\" \"php artisan pail --timeout=0\" \"npm run dev\" --names=server,queue,logs,vite"
        ],
        "test": [
            "@php artisan config:clear --ansi",
            "@php artisan test"
        ],
        "format": [
            "vendor/bin/pint"
        ],
        "format:check": [
            "vendor/bin/pint --test"
        ],
        "analyze": [
            "@format:check",
            "@test"
        ],
        "fresh": [
            "@php artisan migrate:fresh --seed"
        ],
        "setup": [
            "@composer install",
            "@php artisan key:generate",
            "@php artisan storage:link",
            "@fresh"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
